<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application  name="offline-udp" owner="serv"/>

    <dubbo:registry protocol="zookeeper" address="${zookeeper.connectionString}" />

    <dubbo:protocol name="dubbo" port="-1"/>

    <dubbo:reference interface="com.push6.proxy.service.OfflineService" id="offlineService" check="false"/>

    <dubbo:reference id="deviceInfoService" interface="com.push6.data.rbac.service.DeviceInfoService" check="false"/>

</beans>